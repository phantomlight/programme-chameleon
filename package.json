    "pretty-hrtime": "~1.0.1",
    "require-dir": "^0.3.0",
    "rimraf": "~2.4.3",
    "laravel-elixir": "^3.4.2",
    "run-sequence": "~1.1.4",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "gifsicle": "3.0.1",
    "jpegtran-bin": "3.0.5",
    "jquery": "~2.1.4",
    "optipng-bin": "3.0.2",
    "pngquant-bin": "3.0.0"
  }
}
