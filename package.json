{
  "name": "gulp-starter",
  "version": "0.0.1",
  "description": "Laravel gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/forddyce/gulp-starter.git"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "private": true,
  "devDependencies": {
    "babelify": "~6.4.0",
    "browser-sync": "~2.9.11",
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "~2.6",
    "gulp-copy": "0.0.2",
    "gulp-csscomb": "^3.0.6",
    "gulp-filesize": "0.0.6",
    "gulp-iconfont": "^5.0.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "~1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-requirejs-optimize": "^0.3.0",
    "gulp-sass": "^2.0.4",
    "gulp-size": "^2.0.0",
